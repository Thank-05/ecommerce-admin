generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Thêm bảng Store (Cửa hàng) ở dưới này
model Store {
  id        String   @id @default(uuid())
  name      String
  userId    String
  createdAt DateTime @default(now())npx prisma db push
  updatedAt DateTime @updatedAt
}